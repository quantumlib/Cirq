{
  "problemMatcher": [
    {
      "owner": "shellcheck",
      "pattern": [
        {
            "regexp": "^In\\s(.+)\\sline\\s(\\d+):$",
            "file": 1,
            "line": 2
        },
        {
          "regexp": ".*"
        },
        {
            "regexp": "SC(\\d+)(\\s\\((note|warning|error)\\))?:\\s(.+)$",
            "code": 1,
            "severity": 3,
            "message": 4,
            "loop": true
        }
      ]
    }
  ]
}

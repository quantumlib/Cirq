{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": ["es2022", "dom"],
    "target": "es2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "build"
  },
  "include": [
    "e2e/**/*.ts",
    "src/**/*.ts"
  ]
}
